{
  "name": "chatroom",
  "version": "1.0.0",
  "scripts": {
    "client": "cd Chatroom && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "client-install": "cd Chatroom && npm install",
    "server-install": "npm install"
  },
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "express": "^4.16.2",
    "socket.io": "^2.4.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
